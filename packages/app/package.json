{
  "name": "@calendar-kit/app",
  "description": "React Native Calendar Kit",
  "version": "3.0.0-alpha.1",
  "keywords": [
    "react-native",
    "ios",
    "android",
    "calendar-kit",
    "calendar-week-view"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/howljs/react-native-calendar-kit.git",
    "directory": "packages/app"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/howljs/react-native-calendar-kit/issues"
  },
  "homepage": "https://github.com/howljs/react-native-calendar-kit#readme",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/commonjs/src/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "bob build",
    "clean": "del lib"
  },
  "dependencies": {
    "@calendar-kit/core": "workspace:^",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "luxon": "^3.4.4",
    "rrule": "^2.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.2.79",
    "@types/react-dom": "~18.2.25",
    "del-cli": "^5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-builder-bob": "^0.29.0",
    "typescript": "~5.3.3"
  },
  "peerDependencies": {
    "expo-haptics": "*",
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*",
    "react-native-haptic-feedback": "*",
    "react-native-reanimated": "*"
  },
  "peerDependenciesMeta": {
    "expo-haptics": {
      "optional": true
    },
    "react-native-haptic-feedback": {
      "optional": true
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  }
}
